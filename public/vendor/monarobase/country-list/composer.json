{
    "name": "monarobase/country-list",
    "description": "List of all countries with names and ISO 3166-1 codes in all languages and data formats for Laravel 4/5",
    "keywords": ["laravel", "countries"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Thuau",
            "email": "jonathan@monarobase.net",
            "homepage": "https://monarobase.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "umpirsky/country-list": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "Monarobase\\CountryList\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Monarobase\\CountryList\\CountryListServiceProvider"
            ],
            "aliases": {
                "Countries": "Monarobase\\CountryList\\CountryListFacade"
            }
        }
    }
}
